<section class="upcoming-vacations">
  <h1>Upcoming Vacations</h1>
  <% current_user.vacations.each do |vacation| %>
    <div id="vacation-<%= vacation.id %>">
    <h2><%= link_to vacation.name %></h2>
    <li><%= vacation.location %></li>
    <li><%= vacation.start_date %></li>
    <li><%= vacation.end_date %></li>
    <li>Hosted By: <%= vacation.host.name %></li>
    </div>
  <% end %>
</section>
